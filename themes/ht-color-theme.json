{
    "name": "hangtou",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.reference": {
            "italic": true
        },
        "*.mutable": {
            "underline": true,
        },
        "*.documentation": {
            "italic": true,
            "bold": true
        },
        "comment": "#5a5b5e",
        "brace": "#5a5b5e",
        "angle": "#5a5b5e",
        "parenthesis": "#5a5b5e",
        "namespace": {
            "foreground": "#5a5b5e",
            "bold": true
        },
        "keyword": "#fb3b06",
        "keyword.crateRoot": {
            "foreground": "#DF7D3C",
            "bold": true
        },
        "keyword.controlFlow": {
            "foreground": "#C45198",
            "bold": true
        },
        "arithmetic": {
            "foreground": "#B37F93",
            "bold": true
        },
        "comparison": {
            "foreground": "#B37F93",
            "bold": true
        },
        /* -------------------------- Kinda Memory Related -------------------------- */
        "*.constant": {
            "foreground": "#DF7D3C",
            "bold": true
        },
        "variable.static": {
            "foreground": "#DF7D3C",
            "bold": true
        },
        "boolean": "#DF7D3C",
        "builtinType": {
            "foreground": "#DF7D3C"
        },
        "number": "#DF7D3C",
        "lifetime": {
            "foreground": "#DF7D3C",
            "bold": true
        },
        /* -------------------------- Further Generization -------------------------- */
        "macro": {
            "foreground": "#af0bf0",
            "bold": true
        },
        "macroBang": {
            "foreground": "#af0bf0",
            "bold": true
        },
        "typeParameter": {
            "foreground": "#af0bf0",
            "bold": true
        },
        /* -------------------------------- Variables ------------------------------- */
        "variable": "#4f4c4c",
        "parameter": "#4f4c4c",
        "property": "#4f4c4c",
        /* --------------------------------- Struct --------------------------------- */
        "struct": {
            "foreground": "#13acc0",
            "bold": true
        },
        "typeAlias": {
            "foreground": "#13acc0"
        },
        /* ---------------------------------- Enum ---------------------------------- */
        "enum": {
            "foreground": "#0054cb",
            "bold": true
        },
        "enumMember": "#0054cb",
        /* ---------------------------------- Self ---------------------------------- */
        "selfKeyword": {
            "foreground": "#05b652",
            "bold": true
        },
        "selfTypeKeyword": {
            "foreground": "#05b652",
            "bold": true
        },
        /* -------------------------------- Functions ------------------------------- */
        "method": {
            "foreground": "#6f5ec6"
        },
        "function": {
            "foreground": "#6f5ec6",
            "bold": true
        },
        /* ------------------------------ Asynchronous ------------------------------ */
        "*.async": {
            "foreground": "#008712",
            "bold": true
        },
        "keyword.async": {
            "foreground": "#008712",
            "bold": true
        },
        /* --------------------------------- Unsafe --------------------------------- */
        "*.unsafe": {
            "foreground": "#ff0000",
            "bold": true
        },
        /* ---------------------------------- Trait --------------------------------- */
        "interface": {
            "foreground": "#6aa105",
            "bold": true
        },
        /* ------------------------------- Proc Macros ------------------------------ */
        "*.attribute": {
            "foreground": "#695730",
            "bold": true
        }
    },
    "tokenColors": [
        /* -------------------------------- Mutables -------------------------------- */
        {
            "scope": [
                "storage.modifier.mut"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        /* ------------------------------- Proc Macros ------------------------------ */
        {
            "scope": [
                "punctuation.brackets.attribute",
                "meta.attribute"
            ],
            "settings": {
                "foreground": "#6d5f41"
            }
        },
        /* ------------------------------- Secondaries ------------------------------ */
        {
            "scope": [
                "comment",
                "punctuation.brackets.curly",
                "punctuation.brackets.angle",
                "punctuation.brackets.round",
                "punctuation",
                "keyword.operator.logical",
                "keyword.operator.namespace",
                "keyword.operator.assignment.equal",
                "keyword.operator.access",
                "keyword.operator.arrow",
                "keyword.operator.key-value",
                "entity.name.namespace",
                "keyword.operator.range"
            ],
            "settings": {
                "foreground": "#5a5b5e"
            }
        },
        /* -------------------------------- Keywords -------------------------------- */
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#b39265"
            }
        },
        /* ------------------------------- Arithmetics ------------------------------ */
        {
            "scope": [
                "keyword.operator.comparison",
                "keyword.operator.math",
                "keyword.operator.assignment",
                "keyword.operator.question"
            ],
            "settings": {
                "foreground": "#b37f93",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#C45198"
            }
        },
        /* -------------------------- Kinda Memory Related -------------------------- */
        {
            "scope": [
                "entity.name.type.numeric",
                "entity.name.type.primitive",
                "variable.other.constant",
                "constant.other.caps",
                "entity.name.type.lifetime",
                "keyword.operator.borrow",
                "keyword.operator.dereference",
                "punctuation.definition.lifetime"
            ],
            "settings": {
                "foreground": "#d96518",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.numeric.decimal",
                "constant.language"
            ],
            "settings": {
                "foreground": "#e36008"
            }
        },
        /* -------------------------- Further Generization -------------------------- */
        {
            "scope": [
                "entity.name.function.macro",
                "variable.other.metavariable.name",
                "keyword.operator.macro.dollar"
            ],
            "settings": {
                "foreground": "#a48a33",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "variable.other.metavariable.specifier",
            "settings": {
                "foreground": "#9e7a04"
            }
        },
        /* -------------------------------- Variables ------------------------------- */
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#10567a"
            }
        },
        /* --------------------------------- Struct --------------------------------- */
        {
            "scope": "entity.name.type.struct",
            "settings": {
                "foreground": "#01485a",
                "fontStyle": "bold"
            }
        },
        /* ---------------------------------- Self ---------------------------------- */
        {
            "scope": "variable.language.self",
            "settings": {
                "foreground": "#13acc0",
                "fontStyle": "bold"
            }
        },
        /* ---------------------------------- Enum ---------------------------------- */
        {
            "scope": "entity.name.type.enum",
            "settings": {
                "foreground": "#124345",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "variable.other.enummember",
            "settings": {
                "foreground": "#124345"
            }
        },
        /* -------------------------------- Functions ------------------------------- */
        {
            "scope": [
                "meta.function.call",
                "meta.function.definition"
            ],
            "settings": {
                "foreground": "#5d6fc2"
            }
        },
        /* --------------------------------- String --------------------------------- */
        {
            "scope": "string",
            "settings": {
                "foreground": "#02725a"
            }
        },
        /* ------------------------------------ - ----------------------------------- */
        {
            "scope": "keyword.declaration.enum",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.declaration.struct",
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold"
            }
        }
    ],
